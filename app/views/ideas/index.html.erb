<%= render "navBar" %>
<div class="card-columns" >
  <% @ideas.each do |idea| %>
    <div class="card ">
      <div class="card-body">
        <h5 class="card-title"><%= link_to idea.title, idea %></h5>
        <p class="card-text"><%= idea.body %></p>
      </div>    
    </div>  
  <% end %>
</div>